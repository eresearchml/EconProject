\begin_body

\begin_layout Title
Economics Project: Factors affecting survivorship rates from respondents
 to the English longitudinal survey 2002-16
\end_layout

\begin_layout Standard
\begin_inset CommandInset toc
LatexCommand tableofcontents

\end_inset


\end_layout

\begin_layout Standard
David Elliott
\end_layout

\begin_layout Standard
MSc Economics
\end_layout

